!------------------!
! XFT !
!------------------!
 
Xft*dpi: 96
!Xft*dpi: 200
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
Xft*rgba: rgb

! cursor !
!Xcursor.theme:/usr/share/cursors/xorg-x11/Silver

!------------------!
! URvxt Stuff !
!------------------!

!URxvt*scrollTtyOutput: false
!URxvt*scrollWithBuffer: true
!set c.size
URxvt.inputMethod: SCIM
URxvt.urgentOnBell: true
URxvt.urlLauncher: /home/wut/bin/urxvturl
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher
URxvt*cutchars: ()*<>[]{|}
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
!URxvt.matcher.pattern.2: .*\\.mkv
!URxvt.matcher.pattern.3: .*\\.avi
!URxvt.visualBell:       false

URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
URxvt.underlineURLs: true
URxvt.perl-lib: /home/wut/.urxvt/
URxvt.perl-ext: selection,mark-yank-urls

!--[term name]--!
!URxvt*termName: rxvt-256color
!URxvt*termName: VT
 
!--[trans and fg/bg]--!
!URxvt*fadeColor: #191919
URxvt*transparent: true
!URxvt*shading: 20
URxvt*depth:24
URxvt*background: #191919
!urxvt*background:         rgba:02f0/04f0/04f0/dddd
!URxvt*background:rgba:0000/0000/0000/0000
!URxvt*foreground: #EEEEEE
!URxvt*background: #353535
URxvt*foreground: #C0C0C0

!URxvt*transparent:true
!URxvt*tintColor:white
!URxvt*shading:20

URxvt.internalBorder:0
URxvt.externalBorder:0

!URxvt*borderless: true
 
!--[font]--!
!URxvt*font: -*-terminus-*-*-*-*-12-*-*-*-*-*-*-u
!URxvt*boldFont: -*-terminus-bold-*-*-*-12-*-*-*-*-*-*-u
!URxvt*font:      xft:DejaVu Sans Mono:pixelsize=10:autohint=true
!URxvt*boldFont:      xft:DejaVu Sans Mono:pixelsize=10:bold:autohint=true
!URxvt*italicFont:    xft:DejaVu Sans Mono:pixelsize=10:italic:autohint=true
!URxvt*boldItalicFont:    xft:DejaVu Sans Mono:pixelsize=10:bold:italic:autohint=true

dzen2.font:         xft:terminus:pixelsize=12

urxvt*font: xft:Terminus (TTF):pixelsize=12,\
            xft:Kochi Gothic:antialias=false,\
            xft:GulimChe
!xft:Caslon:style=Roman:hinting=false:antialias=false
!urxvt*font: xft:DejaVu Sans Mono:pixelsize=12
!URxvt*boldFont:     xft:terminus:size=16 \
!xft:Kochi Gothic \
!xft:GulimChe
!Baekmuk Gulim

!URxvt*font:         xft:Kochi Gothic:pixelsize=12
!--[scrolling]--!
URxvt*scrollBar: false
URxvt*saveLines: 10000
 
!--[tabs]--!
!URxvt*perl-ext-common: default,tabbed
 
!--[cursor]--!
URxvt*cursorBlink: true

